{
  "name": "wedding-card",
  "version": "0.1.0",
  "homepage": "https://kyounghun-joo.github.io/nextjs-wedding-invite/",
  "scripts": {
    "start:dev": "nodemon",
    "deploy": " touch out/.nojekyll && gh-pages -d out",
    "serve": "npx http-server out",
    "build": "NODE_OPTIONS=--openssl-legacy-provider next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "export": "NODE_OPTIONS=--openssl-legacy-provider  next export",
    "cli": "pankod-cli add",
    "storybook": "start-storybook -s ./public/static -p 6006",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout",
    "dev": "next dev"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.3",
    "@reduxjs/toolkit": "^2.2.5",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "cross-env": "^7.0.3",
    "esm": "^3.2.25",
    "express": "^4.19.2",
    "gh-pages": "^6.1.1",
    "i18next": "^23.11.5",
    "next": "14.2.3",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^15.3.0",
    "next-redux-wrapper": "^8.1.0",
    "next-routes": "^1.4.2",
    "nodemon": "^3.1.2",
    "react": "^18",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18",
    "react-i18next": "^14.1.2",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^3.1.0",
    "sass": "^1.77.4",
    "styled-components": "^6.1.11",
    "swiper": "^11.1.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.1.5",
    "@storybook/addon-knobs": "^8.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "add": "^2.0.6",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "yarn": "^1.22.22"
  }
}
